<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextAppearance.Compat.Notification" parent="@android:style/TextAppearance.Material.Notification" />
    <style name="TextAppearance.Compat.Notification.Info" parent="@android:style/TextAppearance.Material.Notification.Info" />
    <style name="TextAppearance.Compat.Notification.Line2" parent="@style/TextAppearance.Compat.Notification.Info" />
    <style name="TextAppearance.Compat.Notification.Time" parent="@android:style/TextAppearance.Material.Notification.Time" />
    <style name="TextAppearance.Compat.Notification.Title" parent="@android:style/TextAppearance.Material.Notification.Title" />
    <style name="Theme.AutofillInlineSuggestion" parent="@android:style/Theme.DeviceDefault">
        <item name="autofillInlineSuggestionChip">@style/Widget.Autofill.InlineSuggestionChip</item>
        <item name="autofillInlineSuggestionEndIconStyle">@style/Widget.Autofill.InlineSuggestionEndIconStyle</item>
        <item name="autofillInlineSuggestionStartIconStyle">@style/Widget.Autofill.InlineSuggestionStartIconStyle</item>
        <item name="autofillInlineSuggestionSubtitle">@style/Widget.Autofill.InlineSuggestionSubtitle</item>
        <item name="autofillInlineSuggestionTitle">@style/Widget.Autofill.InlineSuggestionTitle</item>
        <item name="isAutofillInlineSuggestionTheme">true</item>
    </style>
    <style name="Widget.Autofill" parent="@android:style/Widget" />
    <style name="Widget.Autofill.InlineSuggestionChip" parent="@style/Widget.Autofill">
        <item name="android:background">@drawable/autofill_inline_suggestion_chip_background</item>
        <item name="android:paddingLeft">13.0dip</item>
        <item name="android:paddingRight">13.0dip</item>
        <item name="android:paddingStart">13.0dip</item>
        <item name="android:paddingEnd">13.0dip</item>
    </style>
    <style name="Widget.Autofill.InlineSuggestionEndIconStyle" parent="@style/Widget.Autofill">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:maxWidth">@dimen/autofill_inline_suggestion_icon_size</item>
    </style>
    <style name="Widget.Autofill.InlineSuggestionStartIconStyle" parent="@style/Widget.Autofill">
        <item name="android:scaleType">fitCenter</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:maxWidth">@dimen/autofill_inline_suggestion_icon_size</item>
    </style>
    <style name="Widget.Autofill.InlineSuggestionSubtitle" parent="@android:style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">#99202124</item>
        <item name="android:layout_marginRight">4.0dip</item>
        <item name="android:layout_marginEnd">4.0dip</item>
    </style>
    <style name="Widget.Autofill.InlineSuggestionTitle" parent="@android:style/TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textColor">#ff202124</item>
        <item name="android:layout_marginLeft">4.0dip</item>
        <item name="android:layout_marginRight">4.0dip</item>
        <item name="android:layout_marginStart">4.0dip</item>
        <item name="android:layout_marginEnd">4.0dip</item>
    </style>
    <style name="Widget.Compat.NotificationActionContainer" parent="">
        <item name="android:background">@drawable/notification_action_background</item>
    </style>
    <style name="Widget.Compat.NotificationActionText" parent="">
        <item name="android:textAppearance">?android:textAppearanceButton</item>
        <item name="android:textSize">@dimen/notification_action_text_size</item>
        <item name="android:textColor">@color/androidx_core_secondary_text_default_material_light</item>
    </style>
</resources>
